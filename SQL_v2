/*!40101 SET @OLD_COLLATION_CONNECTION=@@COLLATION_CONNECTION */;
/*!40101 SET NAMES utf8mb4 */;

--
-- Base de datos: `reagloapi`
--

CREATE TABLE `usuarios` (
  `id_usuario` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(250) NOT NULL UNIQUE,
  `mail` varchar(250) NOT NULL,
  `fecha_alta` date NOT NULL,
  `estado` date NOT NULL,
  `usuario` varchar(15) NOT NULL UNIQUE,
  `password` varchar(15) NOT NULL,
  PRIMARY KEY (`id_usuario`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `grupos` (
  `id_grupo` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(250) NOT NULL UNIQUE,
  `descripcion` varchar(200),
  `fecha_alta` date NOT NULL,
  `fecha_tope` date NOT NULL,
  `agasajado` varchar(100) NOT NULL,
  `monto_total` float NOT NULL,
  PRIMARY KEY (`id_grupo`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `roles` (
  `id_rol` int(11) NOT NULL AUTO_INCREMENT,
  `nombre` varchar(100) NOT NULL UNIQUE,
  `estado` varchar(50) NOT NULL,
  `fecha_alta` date NOT NULL,
  `prefijo` varchar(1) NOT NULL UNIQUE,
  `admin_sistema` tinyint(1) NOT NULL,
  PRIMARY KEY (`id_grupo`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

CREATE TABLE `participantes` (
  `id_usuario` int(11) NOT NULL,
  `id_rol` int(11) NOT NULL,
  `id_grupo` int(11) NOT NULL,
  `fecha_deposito` date,
  `monto_deposito` float,
  --medio_de_pago fk a tabla medios de pago del patron de dise√±o
  PRIMARY KEY (`id_usuario`,`id_rol`,`id_grupo`),
  FOREIGN KEY (`id_usuario`) REFERENCES `usuarios`(`id_usuario`),
  FOREIGN KEY (`id_rol`) REFERENCES `roles`(`id_rol`),
  FOREIGN KEY (`id_grupo`) REFERENCES `grupos`(`id_grupo`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;


